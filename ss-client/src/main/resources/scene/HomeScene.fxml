<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rp.HomeSceneClientController">
    <children>
        <ImageView fx:id="imageView" fitHeight="300.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" />
        <Label fx:id="statusLabel" alignment="CENTER" layoutX="286.0" layoutY="300.0" prefHeight="40.0" prefWidth="600.0" text="Label" AnchorPane.leftAnchor="0.0">
            <font>
                <Font name="Consolas Bold" size="12.0" />
            </font>
        </Label>
        <Button layoutX="108.0" layoutY="348.0" mnemonicParsing="false" onAction="#start" prefHeight="40.0" prefWidth="94.0" text="Start" AnchorPane.leftAnchor="120.0">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
        <Button layoutX="447.0" layoutY="348.0" mnemonicParsing="false" onAction="#stop" prefHeight="40.0" prefWidth="94.0" text="Stop" AnchorPane.rightAnchor="120.0">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
    </children>
</AnchorPane>
